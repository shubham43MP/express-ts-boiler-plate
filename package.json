{
  "name": "boilerplate-ts",
  "version": "1.0.0",
  "description": "An express boiler plate with TS",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "dev": "ts-node-dev src/server.ts",
    "deb:migratiionsample": "prisma migrate dev --name add-last-login-to-user",
